/* pages/index/index.wxss */
.header{
    position: relative;
}
.logo{
    width: 100px;
    height: 100px;
    border-radius: 50%;
    position: absolute;
    z-index: 100;
    left: 50%;
    transform: translateX(-50%);
    border: 1px solid rgb(136,136,136);
    top: 57px;
    overflow: hidden;
}
.image-btn{
    width: 100px;
    height: 100px;
}
.name{
    position:absolute;
    z-index: 300;
    font-size: 16px;
    font-weight: bold;
    left: 50%;
    transform: translateX(-50%);
    color: #333;
    top: 166px;
}
.start{
    position:absolute;
    left: 50%;
    transform: translateX(-50%);
    top: 236px;
    height: 30px !important;
    line-height: 30px !important;
}
