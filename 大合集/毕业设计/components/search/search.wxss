/* conponents/search/search.wxss */
.header{
    width: 100%;
    height:100rpx;
    display: flex;
    padding-left:20rpx;
    padding-right:20rpx;
    align-items: center;
    border-top: 1rpx solid #eee;
    border-bottom: 1rpx solid #eee;
    flex-direction: row;
    position: fixed;
    left: 0;
    top: 0;
    z-index: 150;
    background: rgb(252, 252, 252);
}
.search{
    
   
    width:530rpx;
    height:70rpx;
    background: rgb(255, 255, 255);
    border-radius:30rpx;
    padding-left: 20rpx;
    display: flex;
    align-items: center;
}
.search input{
    flex:1;
    margin-left: 20rpx;
}
.cancel{
    height:65rpx;
    border-radius: 30rpx;
    line-height: 65rpx;
    border-color: #888;
}
.search-container{
    margin-top: 150rpx;
    padding: 20rpx;
}
.searchTitle{
    display: flex;
    height:90rpx;
    align-items: center;
}
.line{
    height:30rpx;
    width:10rpx;
    background: #333;
    margin-right: 15px;
}

.list{
    margin-top: 150rpx;
    padding-left:90rpx;
    padding-right:90rpx;
    display: flex !important;
    flex-wrap: wrap;
    justify-content: space-between !important;
}
v-book{
    margin-bottom: 60rpx;
}
.space{
    display: block;
    position: fixed;
    top:50%;
    width: 100%;
    z-index: 300;
    text-align: center;
}
.scroll-view{
    height:500px;
}
