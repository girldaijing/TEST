/* component/music/index.wxss */
.pic{
    text-align: center;
}
.back{
    width: 200px;
    height: 200px;
    border-radius: 50%;
}
.btn{
    width: 50px;
    height: 50px;
    position: absolute;
    top: 78px;
    left: 134px;
}
.words{
    margin-top: 40px;
    display: flex;
    flex-direction: column;
    align-items: center;
}
.rotate{
    animation: rotate 12s infinite;
}
@keyframes rotate{
    from{
        transform: rotate(0deg)
    }
    to{
        transform: rotate(360deg)
    }
}
